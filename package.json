{
    "name": "sass-code-kit",
    "main": "./lib/sass-code-kit",
    "version": "1.3.0",
    "description": "Compile Sass and Prefix CSS",
    "keywords": [
        "css",
        "scss",
        "sass",
        "prefix",
        "prefixer"
    ],
    "activationHooks": [
        "language-sass:grammar-used"
    ],
