    "license": "MIT",
    "engines": {
        "atom": ">=1.0.0"
    },
    "dependencies": {
        "autoprefixer": "~9.7.0",
        "postcss": "~7.0.0",
        "postcss-scss": "~2.0.0",
        "postcss-sorting": "~5.0.0",
        "sass": "~1.8.0"
    }
}
